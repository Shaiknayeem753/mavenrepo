pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
        stage('git clone') {
            steps {
                git changelog: false, poll: false, url: 'https://github.com/Shaiknayeem753/mavenrepo.git'
            }
        }
        stage('biuld') {
            steps {
               sh 'mvn package'
            }
        }
    }
        
}
